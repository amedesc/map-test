{{#each earthquakes}}
<div  id="{{id}}" class="card-container " >
	<div class="card">
		<div class="card-content">
			<div class="row">
				<div class="col s3 earthquake-image {{color_earthquake}}-icon">
				</div>
				<div class="col s3">
					<p class="magnitude-text">{{intensity}}</p>
					<p class="unit-text">Mw</p>
				</div>
				<div class="col s1">
					<div class="left-divider">
					</div>
				</div>
				<div class="col s5">
					<p class="time-text">Hace {{time_since}}</p>
					<p class="location-text">{{location}}</p>
				</div>
			</div>
		</div>

		<div class="card-action">
			<div class=row>
				<div class="col s8">
					<a href="/mapa_promediado/{{id}}">Mapa Promediado</a>
				</div>
				<div class="col s4">
					<a href="/reporte/{{id}}">Reportar</a>
				</div>
			</div>

		</div>

	</div>
</div>
{{/each }}